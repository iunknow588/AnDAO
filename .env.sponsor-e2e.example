# Sponsor whitelist E2E dedicated env
# Copy to .env.sponsor-e2e.local and fill values before running:
# npm run sponsor:e2e

SPONSOR_E2E_CHAIN_ID=43113
SPONSOR_E2E_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc
SPONSOR_E2E_APPLICATION_REGISTRY_ADDRESS=
SPONSOR_E2E_SPONSOR_PRIVATE_KEY=
SPONSOR_E2E_TARGET_CONTRACT_ADDRESS=
SPONSOR_E2E_OWNER_ADDRESS=
# Optional; if empty uses 0x000...000
SPONSOR_E2E_EOA_ADDRESS=
# Optional sentinel entries used for "remove target then block" verification.
SPONSOR_E2E_SENTINEL_CONTRACT_ADDRESS=0x1000000000000000000000000000000000000001
SPONSOR_E2E_SENTINEL_OWNER_ADDRESS=0x2000000000000000000000000000000000000002
# true: auto call registerSponsor if sponsor not active
SPONSOR_E2E_AUTO_REGISTER=false
