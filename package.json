{
  "name": "@andaowallet/h5",
  "version": "0.1.0",
  "description": "AnDaoWallet HTML5 version - Smart Contract Wallet PWA based on Account Abstraction (ERC-4337)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "typechain": "echo 'TypeChain script disabled for Vercel deployment. Contract types are defined locally in src/utils/kernel-types.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "testnet:verify": "tsx scripts/testnet-verification.ts",
    "pwa:verify": "tsx scripts/pwa-verification.ts",
    "prepare": "husky install",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "tsx scripts/security-audit.ts",
    "lint:security": "eslint src --ext ts,tsx --config .eslintrc.security.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^3.4.3",
    "styled-components": "^6.1.1",
    "viem": "^2.0.0",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-plugin-pwa": "^0.17.4",
    "@typechain/ethers-v6": "^0.5.1",
    "typechain": "^8.3.2",
    "vitest": "^1.0.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jsdom": "^23.0.0",
    "tsx": "^4.7.0"
  }
}

