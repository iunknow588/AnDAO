{
  "name": "@andaowallet/h5",
  "version": "0.1.0",
  "description": "AnDaoWallet HTML5 version - Smart Contract Wallet PWA based on Account Abstraction (ERC-4337)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "typechain": "echo 'TypeChain script disabled for Vercel deployment. Contract types are defined locally in src/utils/kernel-types.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "testnet:verify": "tsx scripts/management/verification/testnet-verification.ts",
    "sponsor:e2e": "node --import tsx scripts/management/verification/sponsor-policy-e2e.ts",
    "pwa:verify": "tsx scripts/management/verification/pwa-verification.ts",
    "mock:indexer": "tsx scripts/functional/mock-application-indexer.ts",
    "bridge:check": "node --import tsx scripts/management/verification/bridge-connectivity-check.ts",
    "bridge:check:local": "node --import tsx scripts/management/verification/bridge-connectivity-check.ts --use-local-mock",
    "deploy:github": "bash scripts/management/deploy/upload-to-github.sh",
    "deploy:vercel": "bash scripts/management/deploy/deploy-vercel.sh",
    "deploy:check": "bash scripts/management/deploy/check-deployment.sh",
    "deploy:all": "bash scripts/management/deploy/deploy-github-and-vercel.sh",
    "dev:local:start": "bash scripts/management/dev/start-local-dev.sh",
    "dev:local:test": "bash scripts/management/dev/test-local-dev.sh",
    "prepare": "node -e \"try{require('husky').install()}catch(e){if(process.env.CI!=='true')console.log(e)}\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "tsx scripts/management/security/security-audit.ts",
    "lint:security": "eslint src --ext ts,tsx --config .eslintrc.security.cjs",
    "verify:standalone": "bash scripts/management/verification/verify-standalone.sh"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "styled-components": "^6.1.1",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^23.0.0",
    "tsx": "^4.7.0",
    "typechain": "^8.3.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.0"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
